---
layout: pages/blog/index.liquid
title: all posts
---

{% header "h3" title %}

<ul>
    {% assign taglist = collections[ 'posts' ] %}
    {% for post in taglist | reverse %}
        {% if post.data.title == 'all posts' %}{% continue %}{% endif %}
        <li><a href="{{ post.url }}">{{ post.data.title }} - {{ post.date | date: "%m&#183;%d&#183;%Y %H:%M %p" }}</a></li>
    {% endfor %}
</ul>